let int x = 10
let int y = 20

sum = x + y
print(sum)

diff = x - y
print(diff)

product = x * y
print(product)

quotient = x / y
print(quotient)

chk (x < y) {
    print("x is less than y")
} alt {
    print("x is not less than y")
}

let int counter = 0
while (counter < 5) {
    print(counter)
    counter = counter + 1
}

for (let int i = 0; i < 10; i = i + 1) {
    print(i)
}
fin
